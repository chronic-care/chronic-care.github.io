            </operand>
            <operand locator="38:36-38:57" name="PSA value 1" libraryName="Prostate" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def locator="40:1-42:16" name="Needs Prostate Cancer Screening Indicator" context="Patient" accessLevel="Public">
         <expression locator="41:3-42:16" xsi:type="If">
            <condition asType="t:Boolean" xsi:type="As">
               <operand locator="41:6-41:38" name="Last PSA value is high" libraryName="Prostate" xsi:type="ExpressionRef"/>
