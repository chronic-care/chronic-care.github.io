      </def>
      <def locator="24:1-25:32" name="Age Between 40 and 70" context="Patient" accessLevel="Public">
         <expression locator="25:3-25:32" xsi:type="And">
            <operand xsi:type="GreaterOrEqual">
               <operand locator="25:3-25:14" precision="Year" xsi:type="CalculateAge">
                  <operand path="birthDate.value" xsi:type="Property">
                     <source name="Patient" xsi:type="ExpressionRef"/>
                  </operand>
               </operand>
               <operand locator="25:24-25:25" valueType="t:Integer" value="40" xsi:type="Literal"/>
            </operand>
            <operand xsi:type="LessOrEqual">
               <operand locator="25:3-25:14" precision="Year" xsi:type="CalculateAge">
                  <operand path="birthDate.value" xsi:type="Property">
                     <source name="Patient" xsi:type="ExpressionRef"/>
                  </operand>
               </operand>
               <operand locator="25:31-25:32" valueType="t:Integer" value="70" xsi:type="Literal"/>
            </operand>
         </expression>
      </def>
      <def locator="27:1-32:3" name="Needs Prostate Cancer Screening" context="Patient" accessLevel="Public">
         <expression locator="28:3-32:3" xsi:type="And">
            <operand locator="28:3-28:25" name="Age Between 40 and 70" xsi:type="ExpressionRef"/>
            <operand locator="29:3-32:3" xsi:type="Or">
               <operand locator="29:4-30:85" xsi:type="Or">
                  <operand locator="29:4-29:33" name="No PSA test results" libraryName="Prostate" xsi:type="ExpressionRef"/>
                  <operand locator="30:8-30:85" xsi:type="And">
                     <operand locator="30:9-30:41" name="Last PSA value is high" libraryName="Prostate" xsi:type="ExpressionRef"/>
                     <operand locator="30:47-30:84" xsi:type="Not">
                        <operand locator="30:51-30:84" name="Has PSA within one year" libraryName="Prostate" xsi:type="ExpressionRef"/>
                     </operand>
                  </operand>
               </operand>
               <operand locator="31:8-31:46" xsi:type="Not">
                  <operand locator="31:12-31:46" name="Has PSA within two years" libraryName="Prostate" xsi:type="ExpressionRef"/>
               </operand>
            </operand>
         </expression>
      </def>
      <def locator="34:1-35:29" name="Needs Prostate Cancer Screening Summary" context="Patient" accessLevel="Public">
         <expression locator="35:3-35:29" valueType="t:String" value="Prostate Cancer Screening" xsi:type="Literal"/>
      </def>
